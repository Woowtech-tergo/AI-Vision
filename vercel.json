{
  "version": 2,
  "builds": [
    {
      "src": "interface/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "interface/app.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt && pip install 'git+https://github.com/facebookresearch/detectron2.git'"
}
